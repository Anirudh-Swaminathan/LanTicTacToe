<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.anirudh.anirudhswami.ticgame.Tic">


    <ImageButton
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/im00"
        android:layout_marginTop="41dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="24dp"
        android:src="@mipmap/yellow"/>

    <ImageButton
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/im01"
        android:layout_alignTop="@+id/im00"
        android:layout_centerHorizontal="true"
        android:src="@mipmap/yellow" />

    <ImageButton
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/im02"
        android:layout_marginEnd="40dp"
        android:layout_alignTop="@+id/im01"
        android:layout_alignParentEnd="true"
        android:src="@mipmap/yellow" />

    <ImageButton
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/im10"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/im00"
        android:layout_alignStart="@+id/im00"
        android:src="@mipmap/yellow" />

    <ImageButton
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/im11"
        android:layout_alignBottom="@+id/im10"
        android:layout_centerHorizontal="true"
        android:src="@mipmap/yellow" />

    <ImageButton
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/im12"
        android:layout_alignTop="@+id/im11"
        android:layout_alignStart="@+id/im02"
        android:src="@mipmap/yellow" />

    <ImageButton
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/im20"
        android:layout_marginTop="36dp"
        android:layout_below="@+id/im10"
        android:layout_alignStart="@+id/im10"
        android:src="@mipmap/yellow" />

    <ImageButton
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/im21"
        android:layout_alignBottom="@+id/im20"
        android:layout_centerHorizontal="true"
        android:src="@mipmap/yellow" />

    <ImageButton
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/im22"
        android:layout_alignTop="@+id/im21"
        android:layout_alignStart="@+id/im12"
        android:src="@mipmap/yellow" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/winner"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="48dp" />

</RelativeLayout>
